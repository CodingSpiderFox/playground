pipeline {
    agent any

    stages {
		stage('Checkout') {
			steps {
				echo 'Checking out scm..'
			}
		}
		
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test B') {
            steps {
                echo 'Testing B..'
            }
        }
		
		stage('Test A') {
            steps {
                echo 'Testing A..'
            }
        }
		
		stage('Test C') {
			steps {
				echo 'Testing C..'
			}
		}
		
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}